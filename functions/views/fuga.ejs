<!doctype html>
  <head>
    <title><%= tag.id %></title>
  </head>
  <body>
  <p><a href="/">/</a></p>
  <p><a href="/tags">tags</a></p>
  <ul>
    <li>
      <a href="/tags/<%= tag.id %>">
        <p>tag : <%= tag.id %></p>
        <p>count : <%= tag.count %></p>
      </a>
    </li>
  </ul>
  <ul>
    <% for (var item of items) { %>
    <li>
      <a href="/item/<%= item.id %>">
        <p>url : <%= item.url %></p>
        <p>repository_url : <%= item.repository_url %></p>
        <p>title : <%= item.title %></p>
        <p>comment : <%- item.comment %></p>
        <% if (item.list) { %>
        <% for (var tag of item.list) { %>
        <p><%= tag %></p>
        <% } %>
        <% } %>
        <% if (item.imageUrl) { %>
        <img src="<%= item.imageUrl %>" alt="">
        <% } %>
      </a>
    </li>
    <% } %>
  </ul>
  </body>
</html>
